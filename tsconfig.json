{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@Mock/*": ["./../__mocks__/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
